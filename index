    $(document).ready(function(){    
        $("#slidedeck_frame").show();
        $("#btn_play_login").fancybox();
        $("#btn_register_beta").fancybox();
        $("#btn_play").click(function(e){
                e.preventDefault();
                $("#maincontent").load($(this).attr("href"));
        });

        if($('.slidedeck').size() > 0){
            var slideDeck = $('.slidedeck').slidedeck({
                scroll: true,
                keys: true,
                cycle: true,
                autoPlay: true,
                autoPlayInterval: 20000, //20 secs
                complete: function(deck){
                    $("#slide_buttons a").removeClass("active");
                    $("#slide_buttons a#slide_lnk_" + (deck.current-1)).addClass("active");
                }
            });
        }

        $("#sdprev").click(function(e){
            e.preventDefault();
            slideDeck.prev();
        });

        $("#sdnext").click(function(e){
            e.preventDefault();
            slideDeck.next();
        });

        $("#slide_buttons a").click(function(e){
            e.preventDefault();
            slideDeck.goTo( $(this).index()+1 );
        });

    });
</script>
